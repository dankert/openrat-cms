<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
  <table>
    <row header="true">
      <column>
        <text value="${message:name}"/>
      </column>
    </row>
    <row class="data">
      <column id="${id}" name="${name}" action="userlist" class="act-clickable">
        <link type="open" id="" action="userlist">
          <image action="user"/>
          <text value="${message:users}"/>
        </link>
      </column>
    </row>
    <row class="data">
      <column id="${id}" name="${name}" action="grouplist" class="act-clickable">
        <link type="open" id="" action="grouplist">
          <image action="group"/>
          <text value="${message:groups}"/>
        </link>
      </column>
    </row>
  </table>
</output>
