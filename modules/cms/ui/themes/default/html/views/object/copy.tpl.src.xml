<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<form>
		<fieldset class="line" label="">
			<part class="label">
				<hidden name="sourceid" default="${sourceId}"/>
			</part>
			<part class="value">
				<text value="${source.name}"/>
			</part>
		</fieldset>
		<fieldset class="line" label="">
			<part class="label">
			</part>
			<part class="value">
				<selectbox name="type" list="types"/>
			</part>
		</fieldset>
		<fieldset class="line" label="">
			<part class="label">
				<hidden name="targetid" default="${targetId}"/>
			</part>
			<part class="value">
				<text value="${target.name}"/>
			</part>
		</fieldset>
	</form>
</output>
