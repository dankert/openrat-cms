<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<form readonly="true">
		<group>
			<fieldset class="line" label="">
				<part class="label">
					<text value="${message:name}"/>
				</part>
				<part class="value">
					<text value="${name}" class="name"/>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<text value="${message:filename}"/>
				</part>
				<part class="value">
					<text value="${filename}" class="filename"/>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<text value="${message:file_extension}"/>
				</part>
				<part class="value">
					<text value="${extension}" class="extension"/>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<text value="${message:description}"/>
				</part>
				<part class="value">
					<text value="${description}"/>
				</part>
			</fieldset>
		</group>
		<group title="${message:additional_info}">
			<fieldset class="line" label="">
				<part class="label">
					<label for="full_filename">
						<text value="${message:full_filename}"/>
					</label>
				</part>
				<part class="value">
					<text value="${full_filename}"/>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<label for="size">
						<text value="${message:FILE_SIZE}"/>
					</label>
				</part>
				<part class="value">
				</part>
				<text value="${size}"/>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<label for="mimetype">
						<text value="${message:FILE_mimetype}"/>
					</label>
				</part>
				<part class="value">
					<text value="${mimetype}"/>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
				</part>
				<part class="value,clickable">
					<link class="action" type="dialog" subaction="size">
						<text value="${message:menu_file_size}"/>
					</link>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<text value="${message:${message:id}}"/>
				</part>
				<part class="value">
					<text value="${objectid}"/>
				</part>
			</fieldset>
			<if present="cache_filename">
				<fieldset class="line" label="">
					<part class="label">
						<label for="cache_filename">
							<text value="${message:CACHE_FILENAME}"/>
						</label>
					</part>
					<part class="value">
						<text value="${cache_filename}"/>
						<newline/>
						<image icon="el_date"/>
						<date date="${cache_filemtime}"/>
					</part>
				</fieldset>
			</if>
			<fieldset class="line" label="">
				<part class="label">
					<label for="pages">
						<text value="${message:FILE_PAGES}"/>
					</label>
				</part>
				<part class="value">
					<table>
						<list list="${pages}" extract="true">
							<row>
								<column>
									<link url="${url}" target="cms_main">
										<image type="page"/>
										<text value="${name}"/>
									</link>
								</column>
							</row>
						</list>
					</table>
					<if empty="${pages}">
						<text value="${message:NOT_FOUND}"/>
					</if>
				</part>
			</fieldset>
		</group>
		<group title="${message:validity}">
			<part class="clickable">
				<link subaction="settings" type="dialog">
					<fieldset class="line" label="">
						<part class="label">
							<text value="${message:state}"/>
						</part>
						<part class="value">
							<if true="${is_valid}">
								<text value="${message:is_yes}"/>
							</if>
							<else>
								<text value="${message:is_no}"/>
							</else>
						</part>
					</fieldset>
					<fieldset class="line" label="">
						<part class="label">
							<text value="${message:from}"/>
						</part>
						<part class="value">
							<date date="${valid_from_date}"/>
						</part>
					</fieldset>
					<fieldset class="line" label="">
						<part class="label">
							<text value="${message:until}"/>
						</part>
						<part class="value">
							<date date="${valid_to_date}"/>
						</part>
					</fieldset>
				</link>
			</part>
		</group>
		<group title="${message:prop_userinfo}">
			<fieldset class="line" label="">
				<part class="label">
					<text value="${message:created}"/>
				</part>
				<part class="value">
					<image elementtype="date"/>
					<date date="${create_date}"/>
					<newline/>
					<image action="user"/>
					<user user="${create_user}"/>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<text value="${message:lastchange}"/>
				</part>
				<part class="value">
					<image elementtype="date"/>
					<date date="${lastchange_date}"/>
					<newline/>
					<image action="user"/>
					<user user="${lastchange_user}"/>
				</part>
			</fieldset>
			<fieldset class="line" label="">
				<part class="label">
					<label for="published_date">
						<text value="${message:published}"/>
					</label>
				</part>
				<part class="value">
					<image elementtype="date"/>
					<date date="${published_date}"/>
					<newline/>
					<image action="user"/>
					<user user="${published_user}"/>
				</part>
			</fieldset>
		</group>
		<group title="${message:settings}">
			<table filter="true">
				<list list="${settings}" value="entry">
					<row class="data">
						<column>
							<text value="${entry.key}"/>
						</column>
						<column class="clickable">
							<link subaction="settings" type="dialog">
								<text value="${entry.value}"/>
							</link>
						</column>
					</row>
				</list>
			</table>
		</group>
	</form>
</output>
