<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<form>
		<logo name="register"></logo>


		<part class="line">
			<part class="label">
				<text value="${message:USER_REGISTER_CODE}"></text>
			</part>
			<part class="value">
				<input name="code" default="" size="25" class="focus"></input>
			</part>
		</part>




		<part class="line">
			<part class="label">
				<text value="${message:USER_USERNAME}"></text>
			</part>
			<part class="value">
				<input type="text" name="username" value="" size="25"></input>
			</part>
		</part>


		<part class="line">
			<part class="label">
				<text value="${message:USER_PASSWORD}"></text>
			</part>
			<part class="value">
				<password name="password" default="" size="25"></password>
			</part>
		</part>


		<part class="line">
			<part class="label">
				<text value="${message:DATABASE}"></text>
			</part>
			<part class="value">
				<selectbox name="dbid" list="dbids" default="actdbid"></selectbox>
			</part>
		</part>

	</form>
</output>