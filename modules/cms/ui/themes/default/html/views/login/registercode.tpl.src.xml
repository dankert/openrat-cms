<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<form>
		<logo name="register"/>

		<fieldset label="${message:USER_REGISTER_CODE}">
			<input name="code" default="" size="25" class="focus"/>
		</fieldset>
		<fieldset label="${message:USER_USERNAME}">
			<input type="text" name="username" value="" size="25"/>
		</fieldset>
		<fieldset label="${message:USER_PASSWORD}">
			<password name="password" default="" size="25"/>
		</fieldset>
		<fieldset label="${message:DATABASE}">
			<selectbox name="dbid" list="dbids" default="actdbid"/>
		</fieldset>
	</form>
</output>
