<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
  <form autosave="true">
    <table>
      <row class="headline">
        <column width="10%"/>
        <column>
          <text value="${message:name}"/>
        </column>
      </row>
      <list list="${memberships}" extract="true">
        <row class="data">
          <column>
            <checkbox name="${var}"/>
          </column>
          <column>
            <label for="${var}">
              <image action="user"/>
              <text value="${name}"/>
            </label>
          </column>
        </row>
      </list>
    </table>
    <row>
      <column colspan="2" class="act">
        <button type="ok"/>
      </column>
    </row>
  </form>
</output>
