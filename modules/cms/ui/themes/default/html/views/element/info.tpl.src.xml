<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<table>
		<row class="data">
			<column colspan="1">
				<text key="type" />
			</column>
			<column>
				<image elementtype="${type}" />
				<text key="${type}" prefix="el_" />
			</column>
		</row>
		<row class="data">
			<column colspan="1">
				<text key="name" />
			</column>
			<column class="clickable">
				<link class="" type="edit" action="element" subaction="prop" id="${id}">
					<text var="name" />
				</link>
			</column>
		</row>
		<row class="data">
			<column colspan="1">
				<text key="id" />
			</column>
			<column>
				<text var="id" />
			</column>
		</row>
	</table>
</output>