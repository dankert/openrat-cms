<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<table>
		<row header="true">
			<column header="true">
				<text value="${message:name}"/>
			</column>
			<column header="true">
				<text value=""/>
			</column>
			<column/>
		</row>
		<list list="${el}" extract="true">
			<row class="data">
				<column class="act-clickable">
					<link type="open" name="${name}" action="model" id="${id}">
						<image action="model"/>
						<text value="${name}" maxlength="25"/>
					</link>
				</column>
				<if false="${is_default}">
					<column class="act-clickable">
						<if present="id">
							<link type="post" action="model" subaction="setdefault" id="${id}">
								<text value="${message:make_default}"/>
							</link>
						</if>
					</column>
				</if>
				<else>
					<column>
						<text value="${message:is_default}" type="emphatic"/>
					</column>
				</else>

				<column>
					<part class="button,button--active-on-hover,toolbar-icon,row--on-hover">
						<image class="menu-icon" menu="more"/>
						<part class="dropdown,button-value">
							<part
								class="dropdown-entry,act-clickable">
								<link type="open" id="${id}" title="${message:menu_open}" action="model">
									<image method="switch"/>
									<text class="dropdown-text" value="${message:menu_open}"/>
								</link>
							</part>
							<part
								class="dropdown-entry,act-clickable">
								<link type="dialog" id="${id}" title="${message:menu_prop_desc}" action="model"  subaction="prop">
									<image method="prop"/>
									<text class="dropdown-text" value="${message:menu_prop}"/>
								</link>
							</part>
							<if false="${is_default}">
								<if present="id">
									<part class="dropdown-entry,act-clickable">
										<link type="post" action="model" subaction="setdefault" id="${id}">
											<text value="${message:make_default}"/>
										</link>
									</part>
								</if>
								<else>
								</else>
							</if>
						</part>
					</part>

				</column>

			</row>
			<set var="select_url"/>
			<set var="default_url"/>
		</list>
	</table>

	<part class="button-knob,act-clickable">
		<link type="dialog" subaction="add">
			<image method="add"/>
			<text value="${message:add}"/>
		</link>
	</part>
</output>
