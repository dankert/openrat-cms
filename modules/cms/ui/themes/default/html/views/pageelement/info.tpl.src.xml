<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<table>
		<row class="data">
			<column>
				<text text="name"></text>
			</column>
			<column class="name">
				<text var="name"></text>
			</column>
		</row>
		<row class="data">
			<column>
				<text text="description"></text>
			</column>
			<column>
				<text var="description"></text>
			</column>
		</row>
		<row class="data">
			<column>
				<text text="type"></text>
			</column>
			<column class="filename">
				<image elementtype="${element_type}"></image>
				<text key="el_${element_type}"></text>
			</column>
		</row>
		<row class="data">
			<column>
				<text key="template"></text>
			</column>
			<column class="clickable">
				<link type="open" action="template" id="${template_id}">
					<image action="template"></image>
					<text var="template_name"></text>
				</link>
			</column>
		</row>
		<row class="data">
			<column>
				<text key="element"></text>
			</column>
			<column class="clickable">
				<link id="${element_id}" name="${element_name}" action="element">
					<image elementtype="${element_type}"></image>
					<text var="element_name"></text>
				</link>
			</column>
		</row>
		<row class="data">
			<column>
				<text key="format" />
				<text key="element" />
			</column>
			<column>
				<text var="element_format" />
			</column>
		</row>
		<row class="data">
			<column>
				<text key="format" />
			</column>
			<column>
				<text var="format" />
			</column>
		</row>
		<row class="data">
			<column>
				<text text="lastchange"></text>
			</column>
			<column>
				<image elementtype="date"></image>
				<date date="${lastchange_date}"></date>
				<text raw=", "></text>
				<image action="user"></image>
				<user user="${lastchange_user}"></user>
			</column>
		</row>
	</table>
</output>