<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<form>
		<part class="line">
			<part class="label">
				<text value="${message:name}"></text>
			</part>
			<part class="value">
				<input name="name" class="focus" maxlength="128"></input>
			</part>
		</part>

		<group title="${message:options}">
			<part class="line">
				<part class="label">
				</part>
				<part class="value">
					<radio name="type" value="empty" checked="true"></radio>
					<label for="type_empty">
						<text value="${message:empty}"></text>
					</label>
					<newline />
					<radio name="type" value="copy"></radio>
					<label for="type_copy">
						<text value="${message:copy}"></text>
					</label>
					<selectbox name="projectid" list="projects" default=""></selectbox>
				</part>
			</part>
		</group>
	</form>
</output>