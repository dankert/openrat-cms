<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<table>
		<row class="headline">
			<column>
				<text value="${message:project}"></text>
			</column>
			<column>
				<text value="${message:filename}"></text>
			</column>
			<column>
				<text value="${message:user_username}"></text>
			</column>
			<column>
				<text value="${message:lastchange}" />
			</column>
		</row>

		<list list="${timeline}" extract="true">
			<row class="data">
				<column class="clickable">
					<link type="post" action="start" subaction="projectmenu" id="${projectid}">
						<text value="${projectname}"></text>
					</link>
				</column>
				<column>
					<text value="${filename}" maxlength="30"></text>
				</column>
				<column>
					<text value="${username}"></text>
				</column>
				<column>
					<date date="${lastchange_date}" />
				</column>

			</row>
		</list>
	</table>
</output>