<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<form>
		<fieldset class="line" label="${message:name}">
			<input name="name" maxlength="50"/>
		</fieldset>
		<group title="${message:options}">
			<fieldset class="line" label="${message:copy}">
				<selectbox name="copytemplateid" list="templates" addempty="true"/>
			</fieldset>
			<!--
          <selectbox name="example" list="examples"/>
          -->
		</group>
	</form>
</output>
