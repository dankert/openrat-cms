<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<header back="true"></header>
	<form>
		<part class="line">
			<part class="label">
				<text value="${message:global_TEMPLATE}"></text>
			</part>
			<part class="input">
				<selectbox name="templateid" default="" list="templates"></selectbox>
			</part>
		</part>


		<part class="line">
			<part class="label">
				<text value="${message:global_NAME}"></text>
			</part>
			<part class="input">
				<input name="name" size="20" class="focus,name"></input>
			</part>
		</part>


		<part class="line">
			<part class="label">
				<text value="${message:global_DESCRIPTION}"></text>
			</part>
			<part class="input">

				<inputarea rows="5" name="description" default=""></inputarea>
			</part>
		</part>

	</form>
</output>