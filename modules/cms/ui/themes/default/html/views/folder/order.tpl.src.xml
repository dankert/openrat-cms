<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
    <form>
        <table class="or-table--sortable">

            <row class="headline">
                <column class="help">
                    <text key="FOLDER_ORDER"></text>
                </column>
                <column class="help">
                    <text key="GLOBAL_TYPE"></text>
                </column>
                <column class="help">
                    <text key="GLOBAL_NAME"></text>
                </column>
                <column class="help">
                    <text key="GLOBAL_FILENAME"></text>
                </column>
                <column class="help">
                    <text key="GLOBAL_LASTCHANGE"></text>
                </column>
            </row>
            <list list="object" extract="true">
                <row class="data" id="${id}">
                    <column>
                        <text raw="_"></text>
                    </column>
                    <column>
                        <text class="sort-value" value="${icon}" />
                        <image action="${icon}"></image>
                    </column>
                    <column>
                        <text var="name"></text>
                    </column>
                    <column>
                        <text var="filename"></text>
                    </column>
                    <column>
                        <date date="${date}"></date>
                    </column>
                </row>
            </list>
        </table>

        <hidden name="order" default=""/>
    </form>
</output>