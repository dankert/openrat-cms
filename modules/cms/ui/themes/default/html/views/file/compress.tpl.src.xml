<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<form>
		<group title="${message:OPTIONS}">
			<part class="line">
				<part class="label">
					<text value="${message:type}"></text>
				</part>
				<part class="input">
					<set var="gz" value="gz"></set>
					<selectbox list="formats" name="format" default="gz"></selectbox>
					<set var="replace" value="1"></set>
					<radio name="replace" value="1"></radio>
					<label for="replace_1">
						<text value="${message:replace}"></text>
					</label>
					<newline></newline>
					<radio name="replace" value="0"></radio>
					<label for="replace_0">
						<text value="${message:new}"></text>
					</label>
				</part>
			</part>
		</group>
	</form>
</output>