<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<group title="${message:prop_userinfo}" open="false">
		<fieldset label="${message:created}">
			<part tag="span">
				<image action="el_date"/>
				<date date="${create_date}"/>
			</part>
			<part tag="span">
				<image action="user"/>
				<user user="${create_user}"/>
			</part>
		</fieldset>

		<fieldset label="${message:lastchange}">
			<part tag="span">
				<image action="el_date"/>
				<date date="${lastchange_date}"/>
			</part>
			<part tag="span">
				<image action="user"/>
				<user user="${lastchange_user}"/>
			</part>
		</fieldset>

		<fieldset label="${message:published}">
			<part tag="span">
				<image action="el_date"/>
				<date date="${published_date}"/>
			</part>
			<part tag="span">
				<image action="user"/>
				<user user="${published_user}"/>
			</part>
		</fieldset>
	</group>
</output>
