<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../template-engine/components/template.xsd">
	<table>
		<row>
			<column class="header">
				<text key="GLOBAL_NAME"></text>
			</column>
			<column class="header">
				<text key="GLOBAL_LASTCHANGE"></text>
			</column>
		</row>
		<list list="result" extract="true">
			<row class="data">
				<column class="clickable">
					<link type="open" action="${type}" name="${name}" id="${id}">
						<image type="${type}"></image>
						<text var="name" title="${desc}"></text>
					</link>
				</column>
				<column>
					<date date="${lastchange_date}"></date>
				</column>
			</row>
		</list>
	</table>
</output>