<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
    <table>
        <row class="headline">
            <column header="true">
                <text value="${message:NAME}"></text>
            </column>
            <column header="true">
                <text value="${message:DESCRIPTION}"></text>
            </column>
            <column header="true">
                <text value="${message:TYPE}"></text>
            </column>
        </row>
        <if empty="${elements}">
            <row>
                <column>
                    <text value="${message:NOT_FOUND}"></text>
                </column>
            </row>
        </if>
        <list list="${elements}" extract="true">
            <row class="data,clickable">
                <column>
                    <link type="open" title="${desc}" name="${name}" action="pageelement" id="${pageelementid}">
                        <image action="pageelement"/>
                        <text value="${label}"></text>
                    </link>
                </column>
                <column title="${desc}">
                    <text value="${desc}"></text>
                </column>
                <column>
                    <image elementtype="${typename}"/>
                    <text value="${message:el_${typename}}"></text>
                </column>
            </row>
        </list>
    </table>

</output>