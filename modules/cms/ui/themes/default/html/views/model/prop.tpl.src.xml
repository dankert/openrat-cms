<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../template-engine/components/template.xsd">
	<form method="post">

		<part class="line">
			<part class="label">
				<text text="GLOBAL_NAME"></text>
			</part>
			<part class="input">
				<input name="name" focus="true" maxlength="50"></input>
			</part>
		</part>

		<part class="line">
			<part class="label">
			</part>
			<part class="input">
				<checkbox name="is_default" readonly="${is_default}"></checkbox>
				<label key="GLOBAL_is_default" for="is_default" />
			</part>
		</part>
	</form>
</output>