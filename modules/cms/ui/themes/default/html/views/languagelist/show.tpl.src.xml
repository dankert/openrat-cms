<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<table>
		<row header="true">
			<column>
				<text value="${message:NAME}"/>
			</column>
			<column>
				<text value="${message:LANGUAGE_ISOCODE}"/>
			</column>
			<column>
				<text value=""/>
			</column>
		</row>
		<list list="${el}" extract="true">
			<row class="data">
				<column class="act-clickable">
					<image action="language"/>
					<link type="open" name="${name}" action="language" id="${id}">
						<text value="${name}" maxlength="25"/>
					</link>
				</column>
				<column>
					<text value="${isocode}"/>
				</column>
				<if false="${is_default}">
					<column class="act-clickable">
						<if present="id">
							<link type="post" action="language" subaction="setdefault" id="${id}">
								<text value="${message:make_default}"/>
							</link>
						</if>
						<else>
						</else>
					</column>
				</if>
				<else>
					<column>
						<text value="${message:is_default}" type="emphatic"/>
					</column>
				</else>
			</row>
			<set var="select_url"/>
			<set var="default_url"/>
		</list>
		<row class="data">
			<column colspan="3" class="act-clickable">
				<link type="dialog" subaction="add">
					<image method="add"/>
					<text value="${message:new}"/>
				</link>
			</column>
		</row>
	</table>
</output>
