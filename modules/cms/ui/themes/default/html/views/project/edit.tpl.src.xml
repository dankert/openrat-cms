<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.openrat.de/template ../../../../../../../template_engine/components/template.xsd">
	<table>
		<row class="headline">
			<column>
				<text value="${message:GLOBAL_TYPE}"></text>
				<text value=" / "></text>
				<text value="${message:GLOBAL_NAME}"></text>
			</column>
		</row>
		<if present="up_url">
			<row class="data">
				<column url="${up_url}">
					<image type="folder_up"></image>
					<text value=".."></text>
				</column>
			</row>
		</if>
		<list list="${content}" extract="true">
			<row class="data">
				<column class="clickable">
					<link type="open" id="${id}" name="${name}" action="${type}">
						<image action="${type}"></image>
						<text value="${message:${name}}"></text>
						<text value=" "></text>
					</link>
				</column>
			</row>
		</list>
	</table>
</output>