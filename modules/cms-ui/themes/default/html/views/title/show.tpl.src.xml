            <if not="" empty="config:login/motd">
                <part class="toolbar-icon menu">
                    <image method="motd"></image>
                    <text class="label" raw="_"/>
                    <part class="arrow arrow-down"/>
                    <part class="dropdown">
                        <part class="entry">
                            <text value="config:login/motd"></text>
