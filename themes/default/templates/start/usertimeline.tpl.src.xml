<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../template.xsd">
	<table>
		<row class="headline">
			<column>
				<text key="project"></text>
			</column>
			<column>
				<text key="filename"></text>
			</column>
			<column>
				<text key="lastchange" />
			</column>
		</row>

		<list list="timeline" extract="true">
			<row class="data">
				<column class="clickable">
					<link type="post" action="start" subaction="projectmenu" id="var:projectid">
						<text var="projectname"></text>
					</link>
				</column>
				<column title="var:filename">
					<text var="filename" maxlength="40"></text>
				</column>
				<column>
					<date date="var:lastchange_date" />
				</column>

			</row>
		</list>
	</table>
</output>